datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

model Movie {
  id Int @id @unique @default(autoincrement())
  url String 
  title String @db.VarChar(100)
  genre String 
  ageGroup Int @map("age_group")
  duration String
  rating Float
  year Int
  description String  

  @@map("movie")
}
 


